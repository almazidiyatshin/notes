{
  "name": "@notes/ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache",
    "lint-fix": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --fix",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "stylelint-fix": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss' --fix",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "preview": "vite preview",
    "prettify": "prettier --log-level warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json"
  },
  "dependencies": {
    "@notes/backend": "workspace:*",
    "@notes/shared": "workspace:*",
    "@sentry/react": "^9.18.0",
    "@tanstack/react-query": "^4.32.6",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.0.1",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-infinite-scroller": "^1.2.6",
    "react-router-dom": "^7.1.5",
    "reset-css": "^5.0.2",
    "sass": "^1.84.0",
    "superjson": "^2.2.2",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^3.4.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.15",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-infinite-scroller": "^1.2.5",
    "@vitejs/plugin-react": "^4.3.4",
    "stylelint": "^16.14.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "vite": "^6.1.0",
    "vite-plugin-svgr": "^4.3.0"
  }
}
