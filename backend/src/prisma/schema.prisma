generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  login     String   @unique
  password  String
  createdAt DateTime @default(now())
}

model Note {
  id        String   @id @default(uuid())
  title     String
  text      String
  createdAt DateTime @default(now())
}
